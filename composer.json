{
  "name": "smarttechtank/larastarter",
  "description": "A Laravel starter package with role-based authentication, API controllers, and more",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Smart Tech Tank",
      "email": "dev@smarttechtank.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/prompts": "^0.1.16 || ^0.2.0 || ^0.3.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "SmartTechTank\\Larastarter\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SmartTechTank\\Larastarter\\LarastarterServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
